generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String  @id
  username    String? @unique
  name        String?
  balance     Int     @default(0)
  centerPoint Float[]
  coins       Coin[]
}

model Coin {
  id       String  @id @default(cuid())
  type     String
  location Float[]
  value    Int

  user   User   @relation(fields: [userId], references: [id])
  userId String
}
